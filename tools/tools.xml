<manifest>
    <!-- build script -->
    <project path="loup/tools" name="bitrvmpd/android_loup_tools" remote="github" revision="loup-n" />

    <!-- Remove builint AOSP compiler -->
    <remove-project name="platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" />

  <!-- Add UberTC toolchain -->
  <project path="prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" name="bitrvmpd/aarch64-linux-android-6.x" remote="github" revision="master" />

  <!-- LineageOS specific packages -->
  <remove-project name="platform/system/sepolicy" />
  <project path="system/sepolicy" name="LineageOS/android_system_sepolicy" groups="pdk" remote="github" />
</manifest>

